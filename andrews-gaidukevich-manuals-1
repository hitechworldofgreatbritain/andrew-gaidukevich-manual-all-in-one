infinity loop bash
---
for (( ; ; ))
do
   echo "Pres CTRL+C to stop..."
   sleep 1
   if (disaster-condition)
   then
	break       	   #Abandon the loop.
   fi
done
---
#!/bin/bash
while :
do
	echo "Press [CTRL+C] to stop.."
	sleep 1
done
