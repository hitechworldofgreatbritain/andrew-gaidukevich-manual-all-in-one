2.
echo $value
3.
mv /media/sf_shared-vpn-folder/ag-trasnlations/temp-reworked-books/book-text/txt-all-in-one/txt-pages-html/page-1s-txt/rename-folder/*.txt "/media/sf_shared-vpn-folder/ag-trasnlations/temp-reworked-books/book-text/txt-all-in-one/txt-pages-html/page-1s-txt/rename-folder/${value}.txt"
