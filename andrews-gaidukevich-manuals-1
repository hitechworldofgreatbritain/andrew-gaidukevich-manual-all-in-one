for i in *; do mkdir /media/sf_shared-vpn-folder/main-tts-work-folder/stack-pages/s1/a1/ready-b1/ready-${i}; done
var0=$(ls /media/sf_shared-vpn-folder/main-tts-work-folder/stack-pages/s1/a1/ready-b1/)
cd /media/sf_shared-vpn-folder/main-tts-work-folder/stack-pages/s1/a1/b1/*
for file in *; do html2text "$file" > "/media/sf_shared-vpn-folder/main-tts-work-folder/stack-pages/s1/a1/ready-b1/$var0/$file.txt"; done
