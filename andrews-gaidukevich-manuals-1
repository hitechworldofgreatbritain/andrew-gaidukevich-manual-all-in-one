i=1
for f in /media/sf_shared-vpn-folder/ag-trasnlations/temp-reworked-books/book-text/txt-all-in-one/txt-pages-html/page-1s-txt/*.txt; do mv $f /media/sf_shared-vpn-folder/ag-trasnlations/temp-reworked-books/book-text/txt-all-in-one/txt-pages-html/page-1s-txt/split-page-$((i++)).txt; done
mv /media/sf_shared-vpn-folder/ag-trasnlations/temp-reworked-books/book-text/txt-all-in-one/txt-pages-html/page-1s-txt/*.txt /media/sf_shared-vpn-folder/ag-trasnlations/temp-reworked-books/book-text/txt-all-in-one/txt-pages-html/page-1s-txt/ready-sep-pages
