wget --continue --recursive --no-parent -R "index.html*" -r https://ftp.nluug.nl/pub/graphics/ -P E:\ALL-GIMP-BLENDER
wget --continue --recursive --limit-rate=200k --no-parent -R "index.html*" -r http://archive.ubuntu.com/ubuntu/ -P U:\ubuntu-archive
wget --continue --recursive --no-parent -R "index.html*" -r http://distro.ibiblio.org/puppylinux/ -P M:\puppy-archive
wget --continue --recursive --no-parent -R "index.html*" -r http://cdimage.ubuntu.com/lubuntu/ -P M:\lubuntu-archive
wget --continue --recursive --no-parent -R "index.html*" -r https://www.google.com/search?q=akihito&sxsrf=AJOqlzXSIwAxYjCAQl60xnerbE5G2ykMsg:1673116520179&source=lnms&tbm=isch&sa=X&ved=2ahUKEwjNnOnejLb8AhUVy4sKHU5QDx4Q_AUoAnoECAEQBA&biw=1920&bih=929&dpr=1 -P U:\akihito
