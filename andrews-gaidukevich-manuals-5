ffmpeg -i input.mkv -filter_complex "[0:a:0][0:a:1][0:a:2][0:a:3][0:a:4][0:a:5] amerge=inputs=6 [out]" -map "0:v" -c:v copy -map "[out]" -c:a aac -b:a 256k -ac 2 output.mkv

