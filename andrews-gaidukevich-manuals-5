---part 1
cp --backup=t /media/sf_shared-vpn-folder-1/main-tts-work-folder/stack-pages/s1/a1/*.txt /media/sf_shared-vpn-folder-1/main-tts-work-folder/stack-pages/s1/temp-a1/temp.txt
var0=$(ls /media/sf_shared-vpn-folder-1/main-tts-work-folder/stack-pages/s1/a1/*/ | tail -1)
var3=$(($var0-2))
awk '/'$var3'/,0' /media/sf_shared-vpn-folder-1/main-tts-work-folder/stack-pages/s1/a1/*.txt | sponge /media/sf_shared-vpn-folder-1/main-tts-work-folder/stack-pages/s1/a1/*.txt
/media/sf_shared-vpn-folder-1/main-tts-work-folder/stack-pages/s1/a1
