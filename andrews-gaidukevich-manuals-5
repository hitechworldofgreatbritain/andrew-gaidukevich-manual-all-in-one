D:\audio-deffence\grep\GnuWin32\bin\grep.exe -Rh refresh M:\sourceforge-archive\zyn\sourceforge.net\ | D:\audio-deffence\grep\GnuWin32\bin\grep.exe -o "https://[^\\?]*" > M:\sourceforge-archive\new-downloads\urllist.txt
D:\audio-deffence\awk\GnuWin32\bin\awk.exe -v prefix="wget --no-check-certificate --continue --recursive --no-parent -r "" "{print prefix $0 suffix}" M:\sourceforge-archive\zyn\urllist.txt" > "M:\sourceforge-archive\zyn\urllist-1.txt"
D:\audio-deffence\awk\GnuWin32\bin\awk.exe -v suffix=""" "{print prefix $0 suffix}" M:\sourceforge-archive\zyn\urllist-1.txt" > "M:\sourceforge-archive\zyn\urllist-2.txt"
D:\audio-deffence\awk\GnuWin32\bin\awk.exe -v suffix=" -T 1 -t 1 -P M:/sourceforge-archive/new-downloads" "{print prefix $0 suffix}" "M:\sourceforge-archive\zyn\urllist-2.txt" > "M:\sourceforge-archive\zyn\urllist-ready.txt"
