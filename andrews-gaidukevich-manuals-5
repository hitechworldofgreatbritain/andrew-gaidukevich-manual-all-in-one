for /f "tokens=1 delims=." %a in ('dir /B *.mp3') do ffmpeg -y -r 0.2 -hwaccel d3d11va -hwaccel_output_format d3d11va -threads 16 -i "%a.mp3" -c copy -shortest -c:v h264_nvenc -shortest "mkv\%a.mp3"
