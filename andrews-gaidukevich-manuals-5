READY INSTRUCTION AND SCRIPT COMMAND FOR FAST FLAC MKV MP3 CONVERTION
---
BATCH CONCAT FLAC
cd /d K:\youtube-mp3-reverb-albums\all-albums\
rmdir /Q /S K:\youtube-mp3-reverb-albums\flac-convertion\text-dir
mkdir K:\youtube-mp3-reverb-albums\flac-convertion\text-dir
(for /f "delims=" %A in ('dir /b /od *.mp3') do @echo 'K:\youtube-mp3-reverb-albums\all-albums\%A') > K:\youtube-mp3-reverb-albums\flac-convertion\text-dir\files.txt
for /f "tokens=*" %a in (K:\youtube-mp3-reverb-albums\flac-convertion\text-dir\files.txt) do (echo file %a) >> K:\youtube-mp3-reverb-albums\flac-convertion\text-dir\filesready.txt
ffmpeg -y -r 0.2 -hwaccel d3d11va -hwaccel_output_format d3d11va -threads 16 -f concat -safe 0 -i "K:\youtube-mp3-reverb-albums\flac-convertion\text-dir\filesready.txt" -c:a copy -c:v h264_nvenc "K:\youtube-mp3-reverb-albums\all-albums\converted-to-flac\output.flac"


CONVERT TO MP3 WAV TO FLAC
for /f "tokens=1 delims=." %a in ('dir /B K:\youtube-mp3-reverb-albums\all-albums\*.wav') do ffmpeg -y -r 0.2 -hwaccel d3d11va -hwaccel_output_format d3d11va -threads 16 -i "K:\youtube-mp3-reverb-albums\all-albums\%a.wav" -c:a flac -b:a 100 -shortest -c:v h264_nvenc -shortest "K:\youtube-mp3-reverb-albums\all-albums\converted-to-flac\%a.flac"

CONVERT TO MKV

for /f "tokens=1 delims=." %a in ('dir /B D:\VMBOX\flacfiles\mkv\flac-1\*.flac') do c:\ffmpeg\bin\ffmpeg.exe -y -r 0.2 -hwaccel d3d11va -hwaccel_output_format d3d11va -threads 16 -loop 1 -i D:\VMBOX\flacfiles\mkv\MAINR.png -i "D:\VMBOX\flacfiles\mkv\flac-1\%a.flac" -c:a copy -b:a 100 -shortest -c:v h264_nvenc -shortest "D:\VMBOX\flacfiles\mkv\mkv-1\%a.mkv"
