ready result
for all files in directory
rmdir /Q /S textfiles
mkdir textfiles
(for /f "delims=" %A in ('dir /b /s /od *.*') do @echo "%A") > textfiles/files.txt
for /f "tokens=*" %a in (textfiles/files.txt) do (echo file %a) > textfiles/filesready.txt

for specific files
rmdir /Q /S textfiles
mkdir textfiles
(for /f "delims=" %A in ('dir /b /s /od *.flac') do @echo "%A") > textfiles/files.txt
for /f "tokens=*" %a in (textfiles/files.txt) do (echo file %a) >> textfiles/filesready.txt
