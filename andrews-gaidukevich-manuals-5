sudo rm -r -f /var/lib/apt/lists
sudo rm -r -f /usr/lib/libreoffice
sudo rm -r -f /usr/lib/firmware
sudo rm -r -f /usr/lib/modules
sudo rm -r -f /usr/share/man
sudo rm -r -f /usr/share/locale
sudo rm -r -f /usr/share/libreoffice
sudo rm -r -f /usr/share/lubuntu/wallpapers
sudo rm -r -f /usr/src
sudo rm -r -f /usr/lib/x86_64-linux-gnu/dri
sudo rm -r -f /usr/lib/x86_64-linux-gnu/libLLVM.so
sudo rm -r -f /usr/share/doc
sudo rm -r -f /usr/share/fonts
sudo rm -r -f /usr/share/groff
sudo rm -r -f /usr/share/icons
sudo apt clean
sudo apt autoremove
---
exit
